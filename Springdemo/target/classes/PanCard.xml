<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            <bean id="myPerson" class="org.jasp.Springdemo.PanCardHolder">
            <constructor-arg name="card" ref="myCard"></constructor-arg>
           </bean>
             <bean id="myCard" class="org.jasp.Springdemo.PanCard">
             <property name="name" value="ghhjjk"></property>
             <property name="number" value="bha1234"></property>
             </bean>
             
            
</beans>